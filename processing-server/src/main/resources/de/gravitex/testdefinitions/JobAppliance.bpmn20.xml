    <userTask id="usertask3" name="Make Appointment" activiti:candidateGroups="humanResources"></userTask>
    <sequenceFlow id="flow6" sourceRef="exclusivegateway2" targetRef="usertask3"></sequenceFlow>
    <userTask id="usertask4" name="Write Rejection" activiti:candidateGroups="humanResources"></userTask>
    <sequenceFlow id="flow7" sourceRef="exclusivegateway2" targetRef="exclusivegateway3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${applierFits == 'false'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="usertask3" targetRef="endevent2"></sequenceFlow>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="flow9" sourceRef="usertask4" targetRef="endevent3"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow10" sourceRef="exclusivegateway3" targetRef="usertask4"></sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="exclusivegateway1" targetRef="exclusivegateway3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${levelOfInterest < 75}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="exclusivegateway1" targetRef="errorendevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${(levelOfInterest < 0) || (levelOfInterest > 100)}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="errorendevent1" name="ErrorEnd">
      <errorEventDefinition errorRef="invalid_level_of_interest"></errorEventDefinition>
    </endEvent>
